<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>commodity-request</h:title><model odk:xforms-version="1.0.0"><instance><data id="commodity-request"><chw_info><chw_name/><cha_name/></chw_info><commodity_type/><quantity/><submission_id/><request_date/><meta><instanceID/></meta></data></instance><instance id="commodities"><root><item><name>MRDTs</name><label>MRDTs</label></item><item><name>AL6s</name><label>AL6s</label></item><item><name>AL12s</name><label>AL12s</label></item><item><name>AL18s</name><label>AL18s</label></item><item><name>AL24s</name><label>AL24s</label></item><item><name>family_planning</name><label>Family Planning Items</label></item><item><name>zinc_tablets</name><label>Zinc Tablets</label></item></root></instance><bind nodeset="/data/chw_info/chw_name" type="string" required="true()"/><bind nodeset="/data/chw_info/cha_name" type="string" required="true()"/><bind nodeset="/data/commodity_type" type="string" required="true()"/><bind nodeset="/data/quantity" type="int" constraint=". &lt;= 99 and . &gt; 0" jr:constraintMsg="Must be between 1 and 99" required="true()"/><bind nodeset="/data/submission_id" type="string" calculate="concat( /data/chw_info/chw_name , '-',  /data/commodity_type , '-', today())"/><bind nodeset="/data/request_date" type="string" calculate="today()"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><group ref="/data/chw_info"><label>CHW Info</label><input ref="/data/chw_info/chw_name"><label>CHW Name</label></input><input ref="/data/chw_info/cha_name"><label>CHA Supervisor</label></input></group><select1 ref="/data/commodity_type"><label>Select commodity type</label><itemset nodeset="instance('commodities')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/quantity"><label>Enter quantity requested</label></input></h:body></h:html>